{
  "name": "elevenlabs-discord-voicebot",
  "description": "Simple demo of ElevenLabs Conversational AI WebSocket that enables Discord uses to talk to LLM's via voice.",
  "main": "dist/bot.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "npm run build && node dist/bot.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@fal-ai/client": "^1.2.3",
    "@types/node": "^22.9.3",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "elevenlabs": "^1.52.0",
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "sodium-native": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
