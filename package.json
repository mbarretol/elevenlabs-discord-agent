{
  "name": "elevenlabs-discord-voicebot",
  "description": "Simple demo of ElevenLabs Conversational AI WebSocket that enables Discord users to talk to LLMs via voice.",
  "main": "dist/bot.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "npm run build && node dist/bot.js",
