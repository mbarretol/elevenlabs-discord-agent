    "@tavily/core": "^0.5.12",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.16.3",
    "dotenv": "^17.2.0",
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "sodium-native": "^5.0.6",
    "utf-8-validate": "^6.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^24.0.14",
    "eslint": "^9.11.1",
    "globals": "^16.3.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
